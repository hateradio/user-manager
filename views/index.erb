<h1>Users <a href="/add" class="btn btn-info">Add</a></h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
      <th>Email</th>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |u| %>
    <tr>
      <td><%= u['id'] %></td>
      <td><%= u['username'] %></td>
      <td><%= u['name'] %></td>
      <td><%= u['email'] %></td>
      <td><a class="btn-sm btn-danger" href="/delete/<%= u['id'] %>">Delete</a></td>
    </tr>
    <% end %>
  </tbody>
</table>
